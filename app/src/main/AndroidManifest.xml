<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.fsc.asimplegame">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.MANAGE_EXTERNAL_STORAGE"></uses-permission>

    <application

        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ASimpleGame"
        tools:targetApi="31">
        <activity
            android:screenOrientation="portrait"
            android:name=".ResultScreen"
            android:exported="false" />
        <activity
            android:screenOrientation="portrait"
            android:name=".questionFiveScreen"
            android:exported="false" />
        <activity
            android:screenOrientation="portrait"
            android:name=".questionFourScreen"
            android:exported="false" />
        <activity
            android:screenOrientation="portrait"
            android:name=".questionThreeScreen"
            android:exported="false" />
        <activity
            android:screenOrientation="portrait"
            android:name=".questionTwoScreen"
            android:exported="false" />
        <activity
            android:screenOrientation="portrait"
            android:name=".questionOneScreen"
            android:exported="false">
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name=".GameInfoScreen"
            android:exported="false" />
        <activity
            android:screenOrientation="portrait"
            android:name=".RegisterScreen"
            android:exported="false" />
        <activity
            android:screenOrientation="portrait"
            android:name=".LogInScreen"
            android:exported="false" />
        <activity
            android:screenOrientation="portrait"
            android:name=".HomeScreen"
            android:exported="false" />
        <activity
            android:screenOrientation="portrait"
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>